{
  "name": "@ethsign/",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EthSign"
  },
  "scripts": {
    "prepare": "husky",
    "clean": "forge clean && hardhat clean && rm -rf cache_hardhat",
    "build": "forge build",
    "build:hh": "hardhat compile",
    "lint:sol": "forge fmt --check && solhint {src,test}/**/*.sol",
    "prettier:check": "prettier --check **/*.{json,md,yml,ts} --ignore-path=.prettierignore",
    "prettier:write": "prettier --write **/*.{json,md,yml,ts} --ignore-path=.prettierignore",
    "test": "forge test",
    "test:coverage": "forge coverage",
    "test:gas": "forge test --gas-report",
    "deploy": "npx hardhat run scripts/02-deploy-simpleerc721merkledistributor.ts --network",
    "deploy:nft": "npx hardhat run scripts/03-deploy-dragonyearnft.ts --network ",
    "merkle:validate": "npx hardhat run scripts/validate_merkle.ts --network ",
    "generate_leaf": "npx hardhat run scripts/generate_leaf.ts --network ",
    "concierge": "npx hardhat run scripts/concierge.ts --network ",
    "validate_merkle_p2": "npx hardhat run scripts/validate_merkle_p2.ts --network ",
    "pause_tranche2_treasury": "npx hardhat run scripts/pause_tranche2_treasury.ts --network ",
    "unlock_token": "npx hardhat run scripts/unlock_token.ts --network ",
    "verify": "npx hardhat verify --network"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@delegatexyz/sdk": "^0.5.2",
    "@ethsign/tokentable-evm-contracts": "2.5.7",
    "@openzeppelin/contracts": "5.0.1",
    "@openzeppelin/contracts-upgradeable": "5.0.1",
    "alchemy-sdk": "^3.3.1",
    "csv-parse": "^5.5.6",
    "ethereum-multicall": "^2.24.0",
    "viem": "^2.17.0",
    "wagmi": "^2.10.9"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.73.0",
    "@nomicfoundation/hardhat-foundry": "^1.1.1",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@openzeppelin/hardhat-upgrades": "^3.0.3",
    "@openzeppelin/merkle-tree": "^1.0.6",
    "@types/node": ">=16.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "dotenv": "^16.4.4",
    "ds-test": "github:dapphub/ds-test",
    "erc721a-upgradeable": "^4.2.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "ethers": "^6.11.1",
    "forge-std": "github:foundry-rs/forge-std#v1",
    "hardhat": "^2.22.5",
    "hardhat-deploy": "^0.12.4",
    "husky": "9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "solhint": "^4.5.4",
    "solidity-docgen": "0.6.0-beta.36",
    "ts-node": "^10.9.2",
    "typescript": "~5.0.4"
  }
}
